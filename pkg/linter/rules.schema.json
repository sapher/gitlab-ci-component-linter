{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": {
    "type": "object",
    "properties": {
      "severity": {
        "type": "string",
        "enum": [
          "error",
          "warning"
        ]
      },
      "message": {
        "type": "string"
      }
    },
    "required": [
      "severity",
      "message"
    ]
  }
}
